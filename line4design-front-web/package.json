{
  "name": "line4design-front-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "postcss ./src/index.pcss -o src/index.css",
    "watch:css": "postcss ./src/index.pcss -o src/index.css -w",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "start": "npm-run-all -p watch:css  start-js",
    "start-mock": "npm-run-all -p watch:css start-js mockAPI mockWS",
    "start-js": "react-scripts start",
    "build-js": "react-app-rewired build --scripts-version react-scripts build",
    "build": "npm-run-all build:css build-js clean:mapfiles",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-i18next": "^8.1.0",
    "@typescript-eslint/parser": "^4.4.1",
    "add": "^2.0.6",
    "eslint": "^7.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.21.4",
    "i18next": "^19.9.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "process-env": "^2.0.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.0",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.8",
    "react-scripts": "4.0.2",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-styled-components": "^1.12.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.3",
    "customize-cra": "^1.0.0",
    "eslint": "^7.19.0",
    "eslint-config-postcss": "^4.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-prettier": "^3.3.1",
