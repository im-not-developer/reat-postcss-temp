    "module-resolver": "^1.0.0",
    "postcss-autoreset": "^3.0.3",
    "postcss-hexrgba": "^2.0.1",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8",
    "tailwindcss": "^2.0.3",
    "web-vitals": "^1.1.0"
  }
